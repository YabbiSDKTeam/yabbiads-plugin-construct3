{
  "languageTag": "ru-RU",
  "fileDescription": "Русификация для YabbiAds",
  "text": {
    "plugins": {
      "yabbi": {
        "name": "YabbiAds",
        "description": "YabbiAds плагин для Construct 3",
        "help-url": "https://github.com/YabbiSDKTeam/yabbiads-plugin-construct3/issues",
        "properties": {
          "publisher-id": {
            "name": "Publisher ID",
            "desc": "Идентификатор издателя. Обязателен для заполнения."
          },
          "interstitial-id": {
            "name": "Interstitial ID",
            "desc": "Идентификатор полноэкранной рекламы. Может оставаться пустой строкой."
          },
          "rewarded-id": {
            "name": "Rewarded ID",
            "desc": "Идентификатор полноэкранной рекламы с вознаграждением. Может оставаться пустой строкой."
          }
        },
        "aceCategories": {
          "interstitial ad": "Полноэкранная реклама",
          "rewarded ad": "Полноэкранная реклама с вознаграждением",
          "setting": "Настройка"
        },
        "conditions": {
          "OnRewardedLoaded": {
            "list-name": "OnRewardedLoaded",
            "display-text": "OnRewardedLoaded",
            "description": "Вызывается когда реклама с загружена"
          },
          "OnRewardedClosed": {
            "list-name": "OnRewardedClosed",
            "display-text": "OnRewardedClosed",
            "description": "Вызывается когда реклама закрыта"
          },
          "OnRewardedFinished": {
            "list-name": "OnRewardedFinished",
            "display-text": "OnRewardedFinished",
            "description": "Вызывается когда реклама закончилась"
          },
          "OnRewardedShowFailed": {
            "list-name": "OnRewardedShowFailed",
            "display-text": "OnRewardedShowFailed",
            "description": "Вызывается если при показе рекламы произошла ошибка"
          },
          "OnRewardedShown": {
            "list-name": "OnRewardedShown",
            "display-text": "OnRewardedShown",
            "description": "Вызывается когда начался показ рекламы"
          },
          "OnRewardedLoadFailed": {
            "list-name": "OnRewardedLoadFailed",
            "display-text": "OnRewardedLoadFailed",
            "description": "Вызывается если в загрузке рекламы произошла ошибка"
          },
          "OnInterstitialLoaded": {
            "list-name": "OnInterstitialLoaded",
            "display-text": "OnInterstitialLoaded",
            "description": "Вызывается когда реклама с загружена"
          },
          "OnInterstitialLoadFailed": {
            "list-name": "OnInterstitialLoadFailed",
            "display-text": "OnInterstitialLoadFailed",
            "description": "Вызывается если в загрузке рекламы произошла ошибка"
          },
          "OnInterstitialShown": {
            "list-name": "OnInterstitialShown",
            "display-text": "OnInterstitialShown",
            "description": "Вызывается когда начался показ рекламы"
          },
          "OnInterstitialShowFailed": {
            "list-name": "OnInterstitialShowFailed",
            "display-text": "OnInterstitialShowFailed",
            "description": "Вызывается если при показе рекламы произошла ошибка"
          },
          "OnInterstitialClosed": {
            "list-name": "OnInterstitialClosed",
            "display-text": "OnInterstitialClosed",
            "description": "Вызывается когда реклама закрыта"
          },
          "IsInterstitialReady": {
            "list-name": "IsInterstitialReady",
            "display-text": "IsInterstitialReady",
            "description": "Проверить, что реклама готова к показу"
          },
          "IsRewardedAdReady": {
            "list-name": "IsRewardedAdReady",
            "display-text": "IsRewardedAdReady",
            "description": "Проверить, что реклама готова к показу"
          },
          "HasUserConsent": {
            "list-name": "HasUserConsent",
            "display-text": "HasUserConsent",
            "description": "Проверить, что пользователь дал разрешение на сбор данных"
          }
        },
        "actions": {
          "LoadRewardedAd": {
            "list-name": "LoadRewardedAd",
            "display-text": "LoadRewardedAd",
            "description": "Загрузить полноэкранную рекламу с вознаграждением"
          },
          "ShowRewardedAd": {
            "list-name": "ShowRewardedAd",
            "display-text": "ShowRewardedAd",
            "description": "Показать полноэкранную рекламу с вознаграждением"
          },
          "ShowInterstitialAd": {
            "list-name": "ShowInterstitialAd",
            "display-text": "ShowInterstitialAd",
            "description": "Показать полноэкранную рекламу"
          },
          "LoadInterstitialAd": {
            "list-name": "LoadInterstitialAd",
            "display-text": "LoadInterstitialAd",
            "description": "Загрузить полноэкранную рекламу"
          },
          "Initialize": {
            "list-name": "Initialize",
            "display-text": "Initialize",
            "description": "Инициализация аддона",
            "params": {
              "PUBLISHER_ID": {
                "name": "Publisher ID",
                "desc": "Идентификатор издателя. Обязателен для заполнения"
              },
              "INTERSTITIAL_ID": {
                "name": "Interstitial ID",
                "desc": "Идентификатор полноэкранной рекламы. Может оставаться пустой строкой"
              },
              "REWARDED_ID": {
                "name": "Rewarded ID",
                "desc": "Идентификатор полноэкранной рекламы с вознаграждением. Может оставаться пустой строкой"
              }
            }
          },
          "SetUserConsent": {
            "list-name": "SetUserConsent",
            "display-text": "SetUserConsent",
            "description": "Установить согласие пользователя на сбор данных",
            "params": {
              "hasConsent": {
                "name": "Разрешение",
                "desc": "Если пользователь дал согласие, установите флаг в \"Разрешил\"",
                "items": {
                  "true": "Разрешил",
                  "false": "Отказался"
                }
              }
            }
          }
        }
      }
    }
  }
}